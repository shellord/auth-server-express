{
  "name": "auth-server-express",
  "version": "1.0.0",
  "scripts": {
    "start": "ts-node src/app.ts",
    "client": "npm run start --prefix client",
    "backend": "ts-node-dev --respawn --transpile-only --exit-child --watch src src/app.ts",
    "dev": "concurrently \"npm run backend\" \"npm run client\""
  },
